                                .button-spin {
                                    display: inline-block;
                                    text-align: center;
                                    vertical-align: middle;
                                    padding: 12px 24px;
                                    border: 1px solid #a12727;
                                    border-radius: 8px;
                                    background: #ff4a4a;
                                    background: linear-gradient(to bottom, #ff4a4a, #992727);
                                    text-shadow: #591717 1px 1px 1px;
                                    font: normal normal bold 20px arial;
                                    color: #ffffff;
                                    text-decoration: none;
                                    position: absolute;
                                    top: 50%;
                                    left: 50%;
                                    -webkit-transform: translate(-50%, -50%);
                                    transform: translate(-50%, -50%);
                                }

                                    .button-spin:hover,
                                    .button-spin:focus {
                                        background: #ff5959;
                                        background: linear-gradient(to bottom, #ff5959, #b62f2f);
                                        color: #ffffff;
                                        text-decoration: none;
                                    }

                                    .button-spin:active {
                                        background: #982727;
                                        background: linear-gradient(to bottom, #982727, #982727);
                                    }

                                    .button-spin:before {
                                        content: "\0000a0";
                                        display: inline-block;
                                        height: 24px;
                                        width: 24px;
                                        line-height: 24px;
                                        margin: 0 4px -6px -4px;
                                        position: relative;
                                        top: 0px;
                                        left: 0px;
                                        background: url("data:image/png;base64,") no-repeat left center transparent;
                                        background-size: 100% 100%;
                                    }
                            </style>

                            <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
                            <script>
                                var giftamount = 5;
                                var gw = $('.gift').outerWidth(true);
                                var giftcenter = gw / 2;
                                var cycle = 7;

                                var containercenter = $('.boxwrapper').outerWidth(true) / 2;
                                for (var i = 0; i <= giftamount; i++) {
                                    var giftduplicate = $('.giftwrapper').children().clone(true, true);
                                    $('.giftwrapper').append(giftduplicate);
                                }

                                $('.button-spin').click(function () {
                                    var btn = $(this);
                                    btn.hide();
                                    //random result
                                    var randomgift = Math.floor((Math.random() * giftamount) + 1);
                                    var distance = (giftamount) * gw * cycle + containercenter + (randomgift * gw) - giftcenter;

                                    $(".giftwrapper").css({ left: "0" });

                                    $('.giftwrapper').animate({ left: "-=" + distance }, 3000, function () {
                                        alert('You Won Gift' + randomgift);
                                        btn.show();
                                    });
                                });
                            </script>

